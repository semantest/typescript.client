  },
  "bugs": "https://github.com/semantest/semantest/issues",
  "dependencies": {
    "axios": "^1.7.2",
    "ws": "^8.14.2",
    "playwright": "^1.40.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "@types/ws": "^8.5.10",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
