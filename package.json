  "description": "TypeScript client SDK for Semantest - AI automation client library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "cli": "ts-node src/cli/cli.ts",
    "build": "tsc",
